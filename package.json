    "mysql": "^2.17.1",
    "mysql2": "^2.1.0",
    "nyc": "^14.1.1",
    "paystack": "^2.0.1",
    "swagger-jsdoc": "^3.5.0",
    "swagger-ui-express": "^4.1.3"
  },
  "devDependencies": {
    "istanbul": "^0.4.5",
    "jasmine": "^3.5.0",
    "jasmine-console-reporter": "^3.1.0",
    "jasmine-supertest": "^1.0.0",
    "nodemon": "^2.0.2"
  }
