  },
  "homepage": "https://github.com/charlesinto/lms-api#readme",
  "dependencies": {
    "@hapi/joi": "^17.1.0",
    "axios": "^0.19.2",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-preset-es2015": "6.24.1",
    "babel-register": "^6.26.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "cors": "^2.8.5",
    "coveralls": "^3.0.9",
